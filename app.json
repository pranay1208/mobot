{
  "expo": {
    "name": "mobot",
    "slug": "mobot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pranay1208.mobot"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/android-chrome-192x192.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.pranay1208.mobot"
    },
    "web": {
      "favicon": "./assets/favicon-32x32.png"
    },
    "plugins": ["@notifee/react-native"]
  }
}
